        function renderCharts(data) {
            const plotlyConfig = { responsive: true, displayModeBar: false };
            const darkLayout = {
                paper_bgcolor: 'rgba(0,0,0,0)',
                plot_bgcolor: 'rgba(0,0,0,0)',
                font: { family: 'Source Sans 3, sans-serif', color: '#a39e93' },
                xaxis: { gridcolor: 'rgba(255,255,255,0.05)', linecolor: 'rgba(255,255,255,0.1)', tickfont: { family: 'JetBrains Mono, monospace', size: 11 } },
                yaxis: { gridcolor: 'rgba(255,255,255,0.05)', linecolor: 'rgba(255,255,255,0.1)', tickfont: { family: 'JetBrains Mono, monospace', size: 11 } },
                margin: { l: 50, r: 20, t: 20, b: 50 }
            };

            // Render distribution charts
            const chartsContainer = document.getElementById('charts-container');
            if (chartsContainer && data.charts && data.charts.length > 0) {
                chartsContainer.textContent = '';
                data.charts.forEach(chart => {
                    const card = document.createElement('div');
                    card.className = 'chart-card';

                    const h3 = document.createElement('h3');
                    h3.textContent = chart.title;
                    card.appendChild(h3);

                    const wrapper = document.createElement('div');
                    wrapper.className = 'chart-wrapper';
                    wrapper.id = chart.id;
                    card.appendChild(wrapper);

                    chartsContainer.appendChild(card);

                    const chartData = JSON.parse(chart.data);
                    const chartLayout = Object.assign({}, JSON.parse(chart.layout), darkLayout);
                    const plotDiv = document.getElementById(chart.id);
                    
                    // Store chart metadata for click handler
                    const chartMetadata = {
                        bin_samples: chart.bin_samples || {},
                        bin_edges: chart.bin_edges || null,
                        title: chart.title
                    };
                    
                    Plotly.newPlot(chart.id, chartData, chartLayout, plotlyConfig).then(function() {
                        // Add click handler for histogram bars
                        if (plotDiv && chartMetadata.bin_samples && chartMetadata.bin_edges && Object.keys(chartMetadata.bin_samples).length > 0) {
                            plotDiv.on('plotly_click', function(eventData) {
                                try {
                                    if (eventData.points && eventData.points.length > 0) {
                                        const point = eventData.points[0];
                                        
                                        // For histograms, point.x gives us the x value (bin center or click location)
                                        if (point.x !== undefined && point.x !== null) {
                                            // Calculate which bin this x value falls into
                                            const xValue = point.x;
                                            const binEdges = chartMetadata.bin_edges;
                                            
                                            // Calculate bin index
                                            let binIdx = null;
                                            if (binEdges.bin_width > 0) {
                                                binIdx = Math.floor((xValue - binEdges.min) / binEdges.bin_width);
                                                binIdx = Math.max(0, Math.min(binIdx, binEdges.nbins - 1));
                                            }
                                            
                                            if (binIdx !== null && chartMetadata.bin_samples[binIdx]) {
                                                const conversation = chartMetadata.bin_samples[binIdx];
                                                openConversationModal(chartMetadata.title + ` (value: ${conversation.value?.toFixed(2) || 'N/A'})`, [conversation]);
                                            } else {
                                                // Try to find the closest bin
                                                let closestBin = null;
                                                let minDist = Infinity;
                                                for (const [binKey, sample] of Object.entries(chartMetadata.bin_samples)) {
                                                    const binNum = parseInt(binKey);
                                                    const binCenter = binEdges.min + (binNum + 0.5) * binEdges.bin_width;
                                                    const dist = Math.abs(xValue - binCenter);
                                                    if (dist < minDist) {
                                                        minDist = dist;
                                                        closestBin = binNum;
                                                    }
                                                }
                                                
                                                if (closestBin !== null && chartMetadata.bin_samples[closestBin]) {
                                                    const conversation = chartMetadata.bin_samples[closestBin];
                                                    openConversationModal(chartMetadata.title + ` (value: ${conversation.value?.toFixed(2) || 'N/A'})`, [conversation]);
                                                }
                                            }
                                        }
                                    }
                                } catch (error) {
                                    console.error('Error handling histogram click:', error);
                                }
                            });
                        }
                    });
                });
            }

            // Render anomaly charts
            const anomalyContainer = document.getElementById('anomaly-charts-container');
            if (anomalyContainer && data.anomaly_charts && data.anomaly_charts.length > 0) {
                anomalyContainer.textContent = '';
                data.anomaly_charts.forEach(chart => {
                    const card = document.createElement('div');
                    card.className = 'chart-card anomaly-card';

                    const h3 = document.createElement('h3');
                    h3.textContent = chart.title;
                    card.appendChild(h3);

                    const badge = document.createElement('span');
                    badge.className = 'anomaly-badge';
                    badge.textContent = chart.outlier_count + ' outliers';
                    card.appendChild(badge);

                    const wrapper = document.createElement('div');
                    wrapper.className = 'chart-wrapper';
                    wrapper.id = chart.id;
                    card.appendChild(wrapper);

                    anomalyContainer.appendChild(card);

                    const chartData = JSON.parse(chart.data);
                    const chartLayout = Object.assign({}, JSON.parse(chart.layout), darkLayout);
                    const plotDiv = document.getElementById(chart.id);
                    
                    // Store chart metadata for click handler
                    const chartMetadata = {
                        outlier_conversations: chart.outlier_conversations || {},
                        outlier_mapping: chart.outlier_mapping || {},
                        title: chart.title
                    };
                    
                    Plotly.newPlot(chart.id, chartData, chartLayout, plotlyConfig).then(function() {
                        // Add click handler for outlier points after chart is rendered
                        if (plotDiv && chartMetadata.outlier_conversations && Object.keys(chartMetadata.outlier_conversations).length > 0) {
                            // Use a closure to capture the metadata
                            plotDiv.on('plotly_click', function(eventData) {
                                try {
                                    // Check if clicked point is from the outlier trace (index 1)
                                    if (eventData.points && eventData.points.length > 0) {
                                        const point = eventData.points[0];
                                        
                                        // Outlier trace is the second trace (index 1)
                                        if (point.curveNumber === 1) {
                                            // Plotly customdata: when customdata is a list of lists,
                                            // point.customdata is the inner list for that specific point
                                            // Format: [message_index, conversation_index]
                                            let conversationIndex = null;
                                            
                                            // Get conversation index from customdata
                                            if (point.customdata !== undefined && point.customdata !== null) {
                                                if (Array.isArray(point.customdata) && point.customdata.length >= 2) {
                                                    // Get the conversation_index (second element)
                                                    conversationIndex = point.customdata[1];
                                                } else if (Array.isArray(point.customdata) && point.customdata.length === 1) {
                                                    // Fallback: if only one element, use it
                                                    conversationIndex = point.customdata[0];
                                                } else if (!Array.isArray(point.customdata)) {
                                                    // If it's not an array, use it directly
                                                    conversationIndex = point.customdata;
                                                }
                                            }
                                            
                                            // Fallback: use messageIndex to look up in outlier_mapping
                                            if ((conversationIndex === null || conversationIndex === undefined) && 
                                                point.customdata && Array.isArray(point.customdata) && 
                                                point.customdata.length > 0 && chartMetadata.outlier_mapping) {
                                                const messageIndex = point.customdata[0];
                                                const msgKey = String(messageIndex);
                                                conversationIndex = chartMetadata.outlier_mapping[msgKey];
                                            }
                                            
                                            if (conversationIndex !== null && conversationIndex !== undefined) {
                                                const convKey = String(conversationIndex);
                                                const conversation = chartMetadata.outlier_conversations[convKey];
                                                
                                                if (conversation) {
                                                    openConversationModal(chartMetadata.title, [conversation]);
                                                } else {
                                                    console.warn('Conversation not found for index:', conversationIndex, 'Available keys:', Object.keys(chartMetadata.outlier_conversations));
                                                }
                                            } else {
                                                console.warn('No conversation index found. Point data:', {
                                                    curveNumber: point.curveNumber,
                                                    customdata: point.customdata,
                                                    pointNumber: point.pointNumber
                                                });
                                            }
                                        }
                                    }
                                } catch (error) {
                                    console.error('Error handling plotly click:', error);
                                }
                            });
                        }
                    });
                });
            }
        }

