                if (conv.reasoning) {
                    const reasoningDiv = document.createElement('div');
                    reasoningDiv.className = 'conversation-reasoning';
                    const reasoningLabel = document.createElement('div');
                    reasoningLabel.className = 'reasoning-label';
                    reasoningLabel.textContent = 'Reasoning:';
                    reasoningDiv.appendChild(reasoningLabel);
                    const reasoningText = document.createElement('div');
                    reasoningText.className = 'reasoning-text';
                    reasoningText.textContent = conv.reasoning;
                    reasoningDiv.appendChild(reasoningText);
                    convItem.appendChild(reasoningDiv);
                }

                convList.appendChild(convItem);
            });

            container.appendChild(convList);
        }

        function closeSampleModal() {
            const modal = document.getElementById('sampleModal');
            modal.classList.remove('active');
            document.body.style.overflow = '';
        }

        // Open conversation modal for a single conversation (used by outlier clicks)
        function openConversationModal(title, conversations) {
            const modal = document.getElementById('sampleModal');
            const modalTitle = document.getElementById('sampleModalTitle');
            const modalBody = document.getElementById('sampleModalBody');

            modal.classList.add('active');
            document.body.style.overflow = 'hidden';

            modalBody.textContent = '';
            modalTitle.textContent = title || 'Conversation';

            renderConversations(conversations, modalBody);
        }
    </script>
</body>
</html>
